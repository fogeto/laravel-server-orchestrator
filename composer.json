{
    "name": "fogeto/laravel-server-orchestrator",
    "description": "Laravel Prometheus monitoring package for multi-project server orchestration. Compatible with Laravel 9-12.",
    "keywords": ["laravel", "prometheus", "monitoring", "metrics", "orchestrator", "observability"],
    "license": "MIT",
    "authors": [
        {
            "name": "FOGETO - Mustafa OÄŸuz BARAN",
            "email": "mustafaoguzbaran@fogeto.com",
            "homepage": "https://fogeto.com",
            "role": "CEO & Lead Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/routing": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/redis": "^9.0|^10.0|^11.0|^12.0",
        "promphp/prometheus_client_php": "^2.2",
        "predis/predis": "^2.0|^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Fogeto\\ServerOrchestrator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fogeto\\ServerOrchestrator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fogeto\\ServerOrchestrator\\Providers\\ServerOrchestratorServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
